#include <iostream>
using namespace std;

class Practice{
    int a,b;
    public:
    Practice(int m,int n){
        a=m;
        b=n;
    }
    
    void display(){
      cout<<"a="<<a<<endl<<"b="<<b;  
    }
};
   

int main() {
    Practice p(8,9);
    p.display();

    return 0;
}

Output:-

a=8
b=9
